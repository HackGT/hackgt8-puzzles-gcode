ServiceStation\";\n  src: url(\"assets/fonts/service-station-display/ServiceStation-Regular.woff\")\n    format(\"woff\");\n  font-weight: normal;\n}\n\n@font-face {\n  font-family: \"ServiceStation\";\n  src: url(\"assets/fonts/service-station-display/ServiceStation-RegularItalic.woff\")\n    format(\"woff\");\n  font-style: italic;\n}\n\n@font-face {\n  font-family: \"ServiceStation\";\n  src: url(\"assets/fonts/service-station-display/ServiceStation-Rough.woff\")\n    format(\"woff\");\n  font-weight: bold;\n}\n\n@font-face {\n  font-family: \"ServiceStation\";\n  src: url(\"assets/fonts/service-station-display/ServiceStation-RoughItalic.woff\")\n    format(\"woff\");\n  font-style: italic;\n  font-weight: bold;\n}\n\n:root {\n  box-sizing: border-box;\n\n  --bg-color: #455483;\n  --bg-color-dark: #252d44;\n  --white: white;\n  --gold: #fbdaaa;\n  --brown: #704633;\n\n  --base-unit: 11px;\n  --base-unit-1: calc(1 * var(--base-unit));\n  --base-unit-2: calc(2 * var(--base-unit));\n  --base-unit-4: calc(4 * var(--base-unit));\n  --base-unit-8: calc(8 * var(--base-unit));\n  --base-unit-16: calc(16 * var(--base-unit));\n\n  --font-primary: Futura, sans serif;\n  --font-secondary: ServiceStation, sans serif;\n}\n\nbody {\n  padding: 0px;\n  margin: 0px;\n  font-family: var(--font-primary);\n  font-size: calc(1.5 * var(--base-unit));\n  color: var(--white);\n  background: var(--bg-color);\n}\n\nh1,\nh2,\nh3,\nh4,\nh5,\nh6 {\n  text-transform: uppercase;\n  font-family: var(--font-secondary);\n}\n\nh1 {\n  font-size: var(--base-unit-8);\n}\n\nh2 {\n  font-size: var(--base-unit-4);\n}\n\nh3 {\n  font-size: var(--base-unit-2);\n}\n\nh4,\nh5,\nh6 {\n  font-size: var(--base-unit-1);\n}\n\nsection {\n  min-height: 100vh;\n}\n\nnav {\n  position: fixed;\n  background: var(--bg-color-dark);\n  font-family: var(--font-secondary);\n  width: 100%;\n}\n\nnav ul {\n  list-style: none;\n  padding: 0px;\n  margin: 0px;\n  display: flex;\n  justify-content: center;\n}\n\nnav li {\n  margin: var(--base-unit-2) var(--base-unit-2);\n}\n\nnav a {\n  color: white;\n  text-decoration: none;\n  text-transform: uppercase;\n}\n\nnav a:visited {\n  color: white;\n}\n\n#home {\n  min-height: 100vh;\n  display: flex;\n  align-items: center;\n  justify-content: center;\n}\n\n#landing-page-layers {\n  position: absolute;\n}\n\n#splash {\n  display: flex;\n  flex-direction: column;\n  justify-content: center;\n  align-items: center;\n  color: var(--gold);\n  font-family: var(--font-secondary);\n}\n\n#splash h1, p {\n  margin: var(--base-unit-1);\n}\n\n#splash p {\n  font-size: var(--base-unit-4);\n}\n\n#tracks img {\n  margin-top: var(--base-unit-4);\n  margin-bottom: var(--base-unit-2);\n}\n\n.btn {\n  font-family: var(--font-primary);\n  text-transform: uppercase;\n  background: var(--gold);\n  color: var(--brown);\n  padding: var(--base-unit-1);\n  border-radius: calc(0.5 * var(--base-unit));\n  transition: all 0.15s ease-in;\n}\n\n.btn:hover {\n  background: var(--brown);\n  color: var(--gold);\n}\n\na.btn {\n  text-decoration: none;\n}\n\n.center {\n  display: flex;\n  flex-direction: column;\n  justify-content: center;\n  align-items: center;\n}\n\nsection.center {\n  margin: var(--base-unit-4) 25%;\n}\n\n.faq {\n  width: 100%;\n  margin-bottom: var(--base-unit-2);\n}\n\n.faq .question {\n  position: relative;\n  display: flex;\n  justify-content: space-between;\n  align-items: center; \n\n  font-family: var(--font-primary);\n  color: var(--white);\n  font-weight: bold;\n  text-transform: uppercase;\n  background: #fff8e14d;\n  padding: var(--base-unit-1) var(--base-unit-2);\n  border-radius: var(--base-unit-2);\n  cursor: pointer;\n  border: none;\n  width: 100%;\n  z-index: 1000;\n}\n\n.faq .answer {\n  position: relative;\n  background: rgba(255, 248, 225, 0.1);\n  border-radius: var(--base-unit-2);\n  padding: var(--base-unit-2);\n  padding-top: var(--base-unit-4);\n  transform: translateY(calc(-1 * var(--base-unit-2)));\n  z-index: 900;\n}\n\n.hide {\n  display: none;\n}\n\nfooter {\n  color: var(--white);\n  display: flex;\n  flex-direction: column;\n  justify-content: center;\n  align-items: center;\n}\n\nfooter .socials ul {\n  list-style: none;\n  padding: 0px;\n  margin: 0px;\n  display: flex;\n}\n\nfooter .socials li {\n  margin: calc(0.25 * var(--base-unit-1)); \n  transition: transform 0.1s ease-out;\n}\n\nfooter .socials li:hover {\n  transform: scale(1.1);\n}\n\nfooter .socials li img {\n  height: calc(1.25 * var(--base-unit-2));\n  width: auto;\n}\n\n.logo {\n  font-size: var(--base-unit-4);\n  display: flex;\n  align-items: center;\n}\n\n.logo img {\n  margin-right: var(--base-unit-1);\n}\n\n.upper-border {\n  border-top: 4px solid black;\n}\n\n.justify-flex-end {\n  justify-content: flex-end;\n}\n\n.pd-1 {\n  padding: var(--base-unit-1);\n}\n\n.pd-2 {\n  padding: var(--base-unit-2);\n}\n\n.pd-4 {\n  padding: var(--base-unit-4);\n}\n\n.pd-8 {\n  padding: var(--base-unit-8);\n}\n\n.pd-16 {\n  padding: var(--base-unit-16);\n}\n\n.pd-top-1 {\n  padding-top: var(--base-unit-1);\n}\n\n.pd-top-2 {\n  padding-top: var(--base-unit-2);\n}\n\n.pd-top-4 {\n  padding-top: var(--base-unit-4);\n}\n\n.pd-top-8 {\n  padding-top: var(--base-unit-8);\n}\n\n.pd-top-16 {\n  padding-top: var(--base-unit-16);\n}\n\n.pd-bot-1 {\n  padding-bottom: var(--base-unit-1);\n}\n\n.pd-bot-2 {\n  padding-bottom: var(--base-unit-2);\n}\n\n.pd-bot-4 {\n  padding-bottom: var(--base-unit-4);\n}\n\n.pd-bot-8 {\n  padding-bottom: var(--base-unit-8);\n}\n\n.pd-bot-16 {\n  padding-bottom: var(--base-unit-16);\n}\n\n/* === delete === */\n.sq {\n  display: block;\n  height: 48px;\n  width: 48px;\n  margin-right: 24px;\n  background: red;\n}\n"},"lineCount":null}},"error":null,"hash":"9831f85fb5321075436b671d76d6b643","cacheData":{"env":{}}}